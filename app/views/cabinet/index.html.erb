<p id="notice"><%= notice %></p>

<h1><%= t('helpers.root') %></h1>
<% @users.each do |user| %>
    <%= user.email %><br>
    <%#= user.full_name %>
    <table class="table table-striped">
      <tbody>
      <% user.tasks.each do |task| %>
          <%= render partial: '/partials/task/task_row', locals: {task: task, is_cabinet: true} %>
      <% end %>
      </tbody>
    </table>
<% end %>