<% provide(:active, 'user') %>
<p id="notice"><%= notice %></p>

<h1><%= @user.full_name %></h1>

<h2><%= @user.email %></h2>

<table class="table table-striped">
  <tbody>
  <% @user.tasks.each do |task| %>
      <%= render partial: '/partials/task/task_row', locals: {task: task, is_cabinet: true} %>
  <% end %>
  </tbody>
</table>